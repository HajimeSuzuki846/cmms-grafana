            "editorMode": "code",
            "format": "table",
            "query": "from(bucket: \"telemetry\")\r\n  |\u003e range(start: v.timeRangeStart, stop: v.timeRangeStop)\r\n  |\u003e filter(fn: (r) =\u003e r.sensor == \"${sensor}\")\r\n  |\u003e aggregateWindow(every: v.windowPeriod, fn: mean, createEmpty: false)\r\n  |\u003e yield(name: \"mean\")\r\n",
            "rawQuery": true,
            "rawSql": "-- ${__interval_ms} はクォートなしで使うこと！\r\n(\r\n  SELECT\r\n    date_bin(INTERVAL '1 second', \"time\", TIMESTAMP '1970-01-01 00:00:00Z') AS \"time\",\r\n    key,\r\n    AVG(value) AS value\r\n  FROM sensor_data\r\n  WHERE sensor_id IN ('$AssetId')\r\n    AND \"time\" \u003e= now() - interval '$Interval'\r\n    AND ${__interval_ms} \u003c 5000      -- \u003c 5s → 1秒ビン（最小）\r\n  GROUP BY 1, key\r\n)\r\nUNION ALL\r\n(\r\n  SELECT\r\n    date_bin(INTERVAL '10 seconds', \"time\", TIMESTAMP '1970-01-01 00:00:00Z') AS \"time\",\r\n    key,\r\n    AVG(value) AS value\r\n  FROM sensor_data\r\n  WHERE sensor_id IN ('$AssetId')\r\n    AND \"time\" \u003e= now() - interval '$Interval'\r\n    AND ${__interval_ms} \u003e= 5000 AND ${__interval_ms} \u003c 15000     -- 5s～\u003c15s → 10秒\r\n  GROUP BY 1, key\r\n)\r\nUNION ALL\r\n(\r\n  SELECT\r\n    date_bin(INTERVAL '30 seconds', \"time\", TIMESTAMP '1970-01-01 00:00:00Z') AS \"time\",\r\n    key,\r\n    AVG(value) AS value\r\n  FROM sensor_data\r\n  WHERE sensor_id IN ('$AssetId')\r\n    AND \"time\" \u003e= now() - interval '$Interval'\r\n    AND ${__interval_ms} \u003e= 15000 AND ${__interval_ms} \u003c 60000    -- 15s～\u003c60s → 30秒\r\n  GROUP BY 1, key\r\n)\r\nUNION ALL\r\n(\r\n  SELECT\r\n    date_bin(INTERVAL '2 minutes', \"time\", TIMESTAMP '1970-01-01 00:00:00Z') AS \"time\",\r\n    key,\r\n    AVG(value) AS value\r\n  FROM sensor_data\r\n  WHERE sensor_id IN ('$AssetId')\r\n    AND \"time\" \u003e= now() - interval '$Interval'\r\n    AND ${__interval_ms} \u003e= 60000 AND ${__interval_ms} \u003c 300000   -- 60s～\u003c300s → 2分\r\n  GROUP BY 1, key\r\n)\r\nUNION ALL\r\n(\r\n  SELECT\r\n    date_bin(INTERVAL '5 minutes', \"time\", TIMESTAMP '1970-01-01 00:00:00Z') AS \"time\",\r\n    key,\r\n    AVG(value) AS value\r\n  FROM sensor_data\r\n  WHERE sensor_id IN ('$AssetId')\r\n    AND \"time\" \u003e= now() - interval '$Interval'\r\n    AND ${__interval_ms} \u003e= 300000 AND ${__interval_ms} \u003c 3600000 -- 5分\r\n  GROUP BY 1, key\r\n)\r\nUNION ALL\r\n(\r\n  SELECT\r\n    date_bin(INTERVAL '1 hour', \"time\", TIMESTAMP '1970-01-01 00:00:00Z') AS \"time\",\r\n    key,\r\n    AVG(value) AS value\r\n  FROM sensor_data\r\n  WHERE sensor_id IN ('$AssetId')\r\n    AND \"time\" \u003e= now() - interval '$Interval'\r\n    AND ${__interval_ms} \u003e= 3600000                                -- 1時間以上\r\n  GROUP BY 1, key\r\n)\r\nORDER BY \"time\";\r\n",
